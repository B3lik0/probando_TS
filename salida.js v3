//constructores en las clases
var Avenger = /** @class */ (function () {
    function Avenger(nombre, equipo) {
        this.nombre = "";
        this.equipo = undefined;
        this.nombre = nombre;
        this.equipo = equipo;
    }
    return Avenger;
}());
var antman = new Avenger("scot", "delta");
console.log(antman);
